{
  "name": "nodejs-server",
  "version": "1.0.0",
  "description": "A simple Node.js server using TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && cp src/keys/* dist/keys",
    "build:prod": "tsc -p tsconfig.prod.json && cp src/keys/* dist/keys && rm dist/environment.js && mv dist/environment.prod.js dist/environment.js",
    "start": "cp src/keys/* dist/keys && node dist/server.js",
    "dev": "cp src/keys/* dist/keys && ts-node src/server.ts",
    "devbuild": "cp src/keys/* dist/keys && tsc -p tsconfig.json && ts-node src/server.ts",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "test:e2e": "playwright test",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.830.0",
    "@aws-sdk/client-rds-data": "^3.828.0",
    "@aws-sdk/lib-dynamodb": "^3.830.0",
    "@types/chai": "^5.2.2",
    "@types/sinon": "^17.0.4",
    "chai": "^5.2.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^4.17.1",
    "express-jwt": "^8.5.1",
    "express-requests-logger": "^4.0.3",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "sinon": "^21.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@jest/globals": "^30.0.5",
    "@playwright/test": "^1.53.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.13",
    "@types/express-requests-logger": "^4.0.0",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mocha": "^10.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^16.11.7",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "mocha": "^11.6.0",
    "node-fetch": "^3.3.2",
    "playwright": "^1.53.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.34.0"
  },
  "author": "",
  "license": "ISC"
}
