<table border="1">
    <thead>
        <tr>
            
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach(function(user, index) { %>
            <tr>
                
                <td><%= user.firstName %></td>
                <td><%= user.lastName %></td>
                <td><%= user.email_address %></td>
                <td>
                    <form action="/admin/set-password" method="get" style="display:inline;">
                        <input type="hidden" name="email_address" value="<%= user.email_address %>">
                        <button type="submit">Set Password</button>
                    </form>
                </td>
            </tr>
        <% }); %>
    </tbody>
</table>